{
  "projectName": "Financial Management System",
  "lastUpdated": "",
  "modules": {
    "backend": {
      "name": "后端服务",
      "description": "API服务、数据库操作、业务逻辑",
      "paths": ["backend/"],
      "excludePaths": ["backend/node_modules/", "backend/dist/", "backend/coverage/"]
    },
    "frontend": {
      "name": "前端界面",
      "description": "用户界面、数据可视化、用户交互",
      "paths": ["frontend/"],
      "excludePaths": ["frontend/node_modules/", "frontend/dist/", "frontend/coverage/"]
    },
    "docs": {
      "name": "项目文档",
      "description": "API文档、开发指南、使用说明",
      "paths": ["docs/"],
      "excludePaths": []
    },
    "scripts": {
      "name": "脚本工具",
      "description": "自动化脚本、部署工具、开发辅助",
      "paths": ["scripts/"],
      "excludePaths": []
    },
    "docker": {
      "name": "容器化配置",
      "description": "Docker配置、容器编排",
      "paths": [".", "backend/", "frontend/"],
      "includeFiles": ["docker-compose.yml", "Dockerfile", "*.dockerfile"],
      "excludePaths": []
    },
    "quantification": {
      "name": "量化统计",
      "description": "项目统计数据、量化分析",
      "paths": ["quantification/"],
      "excludePaths": []
    }
  },
  "fileExtensions": {
    "typescript": [".ts", ".tsx"],
    "javascript": [".js", ".jsx"],
    "json": [".json"],
    "markdown": [".md"],
    "sql": [".sql"],
    "yaml": [".yml", ".yaml"],
    "dockerfile": ["Dockerfile", ".dockerfile"],
    "shell": [".sh", ".ps1"],
    "css": [".css"],
    "html": [".html"],
    "config": [".config.js", ".config.ts", "tsconfig.json", "package.json", ".env", ".example"]
  },
  "countOptions": {
    "includeComments": true,
    "includeBlankLines": true,
    "calculateComplexity": false
  }
} 